{
  "name": "cms-sanity",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && npx @sanity/cli build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && npx @sanity/cli start",
    "postinstall": "lerna bootstrap",
    "lint": "prettier --check \"**/*.ts(x)\"",
    "lint:fix": "prettier --write \"**/*.ts(x)\"",
    "ci": "npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.18.0",
    "@sanity/image-url": "^1.0.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "next": "^11.1.2",
    "next-sanity": "^0.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.6",
    "swr": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@sanity-codegen/types": "^1.0.0-alpha.0",
    "@sanity/cli": "^2.18.0",
    "@types/express": "^4.17.13",
    "husky": "^7.0.2",
    "lerna": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.0",
    "tailwindcss": "^2.2.13",
    "typescript": "^4.4.2"
  }
}
